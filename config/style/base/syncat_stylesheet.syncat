import "../colours.syncat";

variable { color: $variable; }
group_name { color: $variable; }

kind { color: $type; }
style name { color: $function; }
color { color: $builtin; }
number, boolean { color: $value; }
string { color: $string; }
regex { language: regex; }

"&", ">", "!", "+", "~", "import" { color: $keyword; }

comment {
  color: $comment;
  italic: true;
}
